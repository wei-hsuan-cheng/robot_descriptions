<?xml version="1.0"?>
<robot name="Elite EC66" xmlns:xacro="http://www.ros.org/wiki/xacro">

	<xacro:arg name="collider" default="convex"/>

	<!-- 包含ec66.xacro文件 -->
	<xacro:include filename="$(find elite_ec_description)/xacro/ec66.xacro"/>
	
	<!-- 调用EC66宏 -->
	<xacro:EC66 name="" collider="$(arg collider)"/>

</robot>
