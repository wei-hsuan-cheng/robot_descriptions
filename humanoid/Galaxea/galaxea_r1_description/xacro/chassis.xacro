          ixy="6.9163E-08"
          ixz="-7.5368E-10"
          iyy="0.0015854"
          iyz="-7.6749E-12"
          izz="0.00089958"/>
      </inertial>
      <visual>
        <geometry>
          <mesh filename="file://$(find galaxea_r1_description)/meshes/wheel_motor.obj"/>
