      <inertial>
        <origin xyz="2.6203E-06 0.0015607 -1.6564E-07"/>
        <mass value="5"/>
        <inertia
          ixx="0.00089952"
          ixy="-6.9072E-08"
          ixz="7.5036E-10"
          iyy="0.0015854"
          iyz="-7.845E-12"
          izz="0.00089958"/>
      </inertial>
      <visual>
        <geometry>
          <mesh filename="file://$(find galaxea_r1_description)/meshes/wheel_motor.obj"/>
