    <!-- Wheel Motor Joint 2: Steer Motor 2 to Wheel Motor 2 -->
    <joint name="wheel_motor_joint2" type="continuous">
      <origin xyz="0 0 -0.09525"/>
      <parent link="steer_motor_link2"/>
      <child link="wheel_motor_link2"/>
      <axis xyz="0 1 0"/>
    </joint>

    <!-- Steer Motor Link 3 -->
    <link name="steer_motor_link3">
      <inertial>
        <origin xyz="-0.00098879 -0.034317 -0.034601"/>
        <mass value="1.2"/>
        <inertia
          ixx="8.8509E-05"
          ixy="3.7728E-06"
          ixz="-4.584E-09"
          iyy="7.5337E-05"
          iyz="-7.0145E-07"
          izz="0.00010162"/>
      </inertial>
      <visual>
        <geometry>
          <mesh filename="file://$(find galaxea_r1_description)/meshes/steer_motor.obj"/>
