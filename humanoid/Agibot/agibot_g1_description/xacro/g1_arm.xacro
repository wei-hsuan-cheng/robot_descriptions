<?xml version="1.0" ?>
<robot name="g1_arm" xmlns:xacro="http://www.ros.org/wiki/xacro">

	<xacro:macro name="G1Arm" params="name direction:=1">
		<xacro:property name="prefix" value="${name + '_' if name else ''}"/>

		<!-- Arm base link -->
		<link name="${prefix}base_link">
			<inertial>
				<origin xyz="${direction*0.00020641} ${-direction*0.00057488} 0.032296"/>
				<mass value="0.37595"/>
				<inertia ixx="0.00024335" ixy="1.2188e-06" ixz="${direction*2.4281e-06}" iyy="0.00024039"
								 iyz="${-direction*6.6932e-06}" izz="0.00032195"/>
			</inertial>
			<visual>
				<geometry>
					<mesh filename="file://$(find agibot_g1_description)/meshes/arm/base_link.obj"/>
				</geometry>
			</visual>
		</link>

		<!-- Arm links -->
		<link name="${prefix}link1">
			<inertial>
				<origin xyz="${direction*0.0012655} -0.00043778 -0.018388"/>
				<mass value="0.4936"/>
				<inertia ixx="0.00075396" ixy="${-direction*2.4144e-06}" ixz="${-direction*1.8364e-06}" iyy="0.00065755"
								 iyz="1.2174e-06" izz="0.00031848"/>
			</inertial>
			<visual>
				<geometry>
					<mesh filename="file://$(find agibot_g1_description)/meshes/arm/link1.obj"/>
				</geometry>
			</visual>
			<collision>
				<geometry>
					<cylinder length="0.16642063483595848" radius="0.05579268228774888"/>
				</geometry>
				<origin rpy="0.0 0.0 ${-direction*1.2566370614359172}"
								xyz="${direction*0.0026814433577366636} 0.0006133775157398935 -0.04071124829351902"/>
			</collision>
		</link>

		<link name="${prefix}link2">
			<inertial>
				<origin xyz="${-direction*0.00044207} -0.082298 0.00019486"/>
				<mass value="0.46245"/>
				<inertia ixx="0.00065719" ixy="3.7112e-07" ixz="-3.6668e-06" iyy="0.00043459" iyz="-2.517e-05"
								 izz="0.00072571"/>
			</inertial>
			<visual>
				<geometry>
					<mesh filename="file://$(find agibot_g1_description)/meshes/arm/link2.obj"/>
				</geometry>
			</visual>
			<collision>
				<origin rpy="1.5707963267948966 0 0" xyz="0.0 -0.044062052 0.0009765625"/>
				<geometry>
					<cylinder length="0.18085919" radius="0.06302423"/>
				</geometry>
			</collision>
		</link>

		<link name="${prefix}link3">
			<inertial>
				<origin xyz="${direction*0.0010558} 4.4416e-05 -0.056128"/>
				<mass value="0.50026"/>
				<inertia ixx="0.0013016" ixy="3.1401e-06" ixz="1.4255e-05" iyy="0.001427" iyz="2.4361e-05" izz="0.00033587"/>
			</inertial>
			<visual>
				<geometry>
					<mesh filename="file://$(find agibot_g1_description)/meshes/arm/link3.obj"/>
				</geometry>
			</visual>
			<collision>
				<origin xyz="${direction*0.0006559932284637068} -2.2628975236450855e-05 -0.07219303026795387"/>
				<geometry>
					<cylinder length="0.22061403840780258" radius="0.05216156143823925"/>
				</geometry>
			</collision>
		</link>

		<link name="${prefix}link4">
			<inertial>
				<origin xyz="2.4212e-05 -0.06248 0.002536"/>
				<mass value="0.27473"/>
				<inertia ixx="0.00029283" ixy="4.5586e-07" ixz="5.8042e-08" iyy="0.00018852" iyz="-1.116e-06" izz="0.00031238"/>
			</inertial>
			<visual>
				<geometry>
					<mesh filename="file://$(find agibot_g1_description)/meshes/arm/link4.obj"/>
				</geometry>
			</visual>
			<collision>
				<origin rpy="1.5707963267948966 0 0" xyz="0.0 -0.03817895 -0.0043945312"/>
				<geometry>
					<cylinder length="0.154955404" radius="0.057891708"/>
				</geometry>
			</collision>
		</link>

		<link name="${prefix}link5">
			<inertial>
				<origin xyz="2.6223e-06 -0.00023041 -0.024141"/>
				<mass value="0.25139"/>
				<inertia ixx="0.00023516" ixy="-6.5868e-09" ixz="-1.9428e-08" iyy="0.00028807" iyz="-1.2108e-05"
								 izz="0.00013068"/>
			</inertial>
			<visual>
				<geometry>
					<mesh filename="file://$(find agibot_g1_description)/meshes/arm/link5.obj"/>
				</geometry>
			</visual>
			<collision>
				<origin xyz="${-direction*0.00022255510134868567} -4.227476414728204e-05 -0.03344946540892124"/>
				<geometry>
					<cylinder length="0.13423806056380272" radius="0.04562028545392604"/>
				</geometry>
			</collision>
		</link>

		<link name="${prefix}link6">
			<inertial>
				<origin xyz="5.0382e-06 -0.086696 0.002556"/>
				<mass value="0.26634"/>
				<inertia ixx="0.00031434" ixy="-1.2113e-09" ixz="1.0011e-07" iyy="0.00014464" iyz="-2.033e-05"
								 izz="0.00032449"/>
			</inertial>
			<visual>
				<geometry>
					<mesh filename="file://$(find agibot_g1_description)/meshes/arm/link6.obj"/>
				</geometry>
			</visual>
			<collision>
				<origin rpy="0.0 -1.4835107730827122 -1.56897465223053"
								xyz="${-direction*1.7782549414354032e-05} -0.05507596972928094 -0.0017447156928267265"/>
				<geometry>
					<cylinder length="0.18272488500982115" radius="0.04857149291994264"/>
				</geometry>
			</collision>
		</link>

		<link name="${prefix}end_link">
			<inertial>
				<origin xyz="-7.5134e-05 -0.00024521 -0.016705"/>
				<mass value="0.06158"/>
				<inertia ixx="1.8407e-05" ixy="3.4818e-08" ixz="-3.2902e-09" iyy="1.818e-05" iyz="1.1326e-07" izz="2.8547e-05"/>
			</inertial>
			<visual>
				<geometry>
					<mesh filename="file://$(find agibot_g1_description)/meshes/arm/link7.obj"/>
				</geometry>
			</visual>
			<collision>
				<origin rpy="3.141592653589793 -1.2246467991473532e-16 3.141592653589793"
								xyz="-4.552953707661759e-05 -0.002300474691790042 -0.02249999972991645"/>
				<geometry>
					<cylinder length="0.045999998692423105" radius="0.030799950382754494"/>
				</geometry>
			</collision>
		</link>

		<!-- Arm joints -->
		<joint name="${prefix}joint1" type="revolute">
			<parent link="${prefix}base_link"/>
			<child link="${prefix}link1"/>
			<origin xyz="0.0 0.0 0.1859"/>
			<axis xyz="0.0 0.0 ${direction*1.0}"/>
			<limit effort="60.0" lower="-3.14" upper="3.14" velocity="3.14"/>
		</joint>

		<joint name="${prefix}joint2" type="revolute">
			<parent link="${prefix}link1"/>
			<child link="${prefix}link2"/>
			<origin rpy="-1.5707963267948966 0.0 ${direction*1.5707963267948966}" xyz="0.0 0.0 0.0"/>
			<axis xyz="0.0 0.0 ${direction*1.0}"/>
			<limit effort="60.0" lower="-2.09" upper="1.48" velocity="3.14"/>
		</joint>

		<joint name="${prefix}joint3" type="revolute">
			<parent link="${prefix}link2"/>
			<child link="${prefix}link3"/>
			<origin rpy="-1.5707963267948966 ${direction*1.5707963267948966} 3.141592653589793" xyz="0.0 -0.305 0.0"/>
			<axis xyz="0.0 0.0 ${direction*1.0}"/>
			<limit effort="60.0" lower="-3.10" upper="3.10" velocity="3.14"/>
		</joint>

		<joint name="${prefix}joint4" type="revolute">
			<parent link="${prefix}link3"/>
			<child link="${prefix}link4"/>
			<origin rpy="-1.5707963267948966 0.0 3.141592653589793" xyz="0.0 0.0 0.0"/>
			<axis xyz="0.0 0.0 ${direction*1.0}"/>
			<limit effort="60.0" lower="-1.48" upper="1.48" velocity="3.14"/>
		</joint>

		<joint name="${prefix}joint5" type="revolute">
			<parent link="${prefix}link4"/>
			<child link="${prefix}link5"/>
			<origin rpy="-1.5707963267948966 0.0 ${-direction*3.141592653589793}" xyz="0.0 -0.1975 0.0"/>
			<axis xyz="0.0 0.0 ${direction*1.0}"/>
			<limit effort="30.0" lower="-3.10" upper="3.10" velocity="3.14"/>
		</joint>

		<joint name="${prefix}joint6" type="revolute">
			<parent link="${prefix}link5"/>
			<child link="${prefix}link6"/>
			<origin rpy="-1.5707963267948966 0.0 0.0" xyz="0.0 0.0 0.0"/>
			<axis xyz="0.0 0.0 ${direction*1.0}"/>
			<limit effort="30.0" lower="-1.74" upper="1.74" velocity="3.14"/>
		</joint>

		<joint name="${prefix}joint7" type="revolute">
			<origin rpy="1.5707963267948966 0.0 0.0" xyz="0.0 -0.1805 0.0"/>
			<parent link="${prefix}link6"/>
			<child link="${prefix}end_link"/>
			<axis xyz="0.0 0.0 ${direction*1.0}"/>
			<limit effort="30.0" lower="-3.10" upper="3.10" velocity="3.14"/>
		</joint>

	</xacro:macro>

</robot>
