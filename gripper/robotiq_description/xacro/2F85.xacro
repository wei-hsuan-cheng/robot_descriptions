<?xml version="1.0" encoding="utf-8"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
  <xacro:macro name="Robotiq85" params="name">

    <xacro:property name="prefix" value="${name + '_' if name else ''}"/>

    <link name="${prefix}gripper_base">
      <inertial>
        <origin xyz="8.625E-08 -4.6583E-06 0.03145"/>
        <mass value="0.22652"/>
        <inertia ixx="0.00020005" ixy="-4.2442E-10" ixz="-2.9069E-10" iyy="0.00017832" iyz="-3.4402E-08"
                 izz="0.00013478"/>
      </inertial>
      <visual>
        <geometry>
          <mesh filename="file://$(find robotiq_description)/meshes/robotiq85/visual/base_link.dae"/>
        </geometry>
      </visual>
      <collision>
        <geometry>
          <mesh filename="file://$(find robotiq_description)/meshes/robotiq85/collision/base_link.stl"/>
        </geometry>
        <material name="black">
          <color rgba="0.1 0.1 0.1 1"/>
        </material>
      </collision>
    </link>

    <link name="${prefix}left_outer_knuckle">
      <inertial>
        <origin xyz="-0.000200000000003065 0.0199435877845359 0.0292245259211331"/>
        <mass value="0.00853198276973456"/>
        <inertia ixx="2.89328108496468E-06" ixy="-1.57935047237397E-19" ixz="-1.93980378593255E-19"
                 iyy="1.86719750325683E-06" iyz="-1.21858577871576E-06" izz="1.21905238907251E-06"/>
      </inertial>
      <visual>
        <geometry>
          <mesh filename="file://$(find robotiq_description)/meshes/robotiq85/visual/outer_knuckle.dae"/>
        </geometry>
      </visual>
      <collision>
        <geometry>
          <mesh filename="file://$(find robotiq_description)/meshes/robotiq85/collision/outer_knuckle.dae"/>
        </geometry>
      </collision>
    </link>

    <link name="${prefix}left_outer_finger">
      <inertial>
        <origin xyz="0.00030115855001899 0.0373907951953854 -0.0208027427000385"/>
        <mass value="0.022614240507152"/>
        <inertia ixx="1.52518312458174E-05" ixy="9.76583423954399E-10" ixz="-5.43838577022588E-10"
                 iyy="6.17694243867776E-06" iyz="6.78636130740228E-06" izz="1.16494917907219E-05"/>
      </inertial>
      <visual>
        <geometry>
          <mesh filename="file://$(find robotiq_description)/meshes/robotiq85/visual/outer_finger.dae"/>
        </geometry>
      </visual>
      <collision>
        <geometry>
          <mesh filename="file://$(find robotiq_description)/meshes/robotiq85/collision/outer_finger.dae"/>
        </geometry>
      </collision>
    </link>

    <link name="${prefix}left_inner_finger">
      <inertial>
        <origin xyz="0.000299999999999317 0.0160078233491243 -0.0136945669206257"/>
        <mass value="0.0104003125914103"/>
        <inertia ixx="2.71909453810972E-06" ixy="1.35402465472579E-21" ixz="-7.1817349065269E-22"
                 iyy="7.69100314106116E-07" iyz="6.74715432769696E-07" izz="2.30315190420171E-06"/>
      </inertial>
      <visual>
        <geometry>
          <mesh filename="file://$(find robotiq_description)/meshes/robotiq85/visual/inner_finger.dae"/>
        </geometry>
      </visual>
      <collision>
        <geometry>
          <mesh filename="file://$(find robotiq_description)/meshes/robotiq85/collision/inner_finger.dae"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}left_inner_finger_pad">
      <visual>
        <geometry>
          <box size="0.022 0.00635 0.0375"/>
        </geometry>
        <material name="white">
          <color rgba="0.9 0.9 0.9 1"/>
        </material>
      </visual>
      <collision>
        <geometry>
          <box size="0.022 0.00635 0.0375"/>
        </geometry>
      </collision>
    </link>

    <link name="${prefix}left_inner_knuckle">
      <inertial>
        <origin xyz="0.000123011831763771 0.0507850843201817 0.00103968640075166"/>
        <mass value="0.0271177346495152"/>
        <inertia ixx="2.61910379223783E-05" ixy="-2.43616858946494E-07" ixz="-6.37789906117123E-09"
                 iyy="2.8270243746167E-06" iyz="-5.37200748039765E-07" izz="2.83695868220296E-05"/>
      </inertial>
      <visual>
        <geometry>
          <mesh filename="file://$(find robotiq_description)/meshes/robotiq85/visual/inner_knuckle.dae"/>
        </geometry>
      </visual>
      <collision>
        <geometry>
          <mesh filename="file://$(find robotiq_description)/meshes/robotiq85/collision/inner_knuckle.dae"/>
        </geometry>
      </collision>
    </link>

    <link name="${prefix}right_outer_knuckle">
      <inertial>
        <origin xyz="-0.000200000000003065 0.0199435877845359 0.0292245259211331"/>
        <mass value="0.00853198276973456"/>
        <inertia ixx="2.89328108496468E-06" ixy="-1.57935047237397E-19" ixz="-1.93980378593255E-19"
                 iyy="1.86719750325683E-06" iyz="-1.21858577871576E-06" izz="1.21905238907251E-06"/>
      </inertial>
      <visual>
        <geometry>
          <mesh filename="file://$(find robotiq_description)/meshes/robotiq85/visual/outer_knuckle.dae"/>
        </geometry>
      </visual>
      <collision>
        <geometry>
          <mesh filename="file://$(find robotiq_description)/meshes/robotiq85/collision/outer_knuckle.dae"/>
        </geometry>
      </collision>
    </link>

    <link name="${prefix}right_outer_finger">
      <inertial>
        <origin xyz="0.00030115855001899 0.0373907951953854 -0.0208027427000385"/>
        <mass value="0.022614240507152"/>
        <inertia ixx="1.52518312458174E-05" ixy="9.76583423954399E-10" ixz="-5.43838577022588E-10"
                 iyy="6.17694243867776E-06" iyz="6.78636130740228E-06" izz="1.16494917907219E-05"/>
      </inertial>
      <visual>
        <geometry>
          <mesh filename="file://$(find robotiq_description)/meshes/robotiq85/visual/outer_finger.dae"/>
        </geometry>
      </visual>
      <collision>
        <geometry>
          <mesh filename="file://$(find robotiq_description)/meshes/robotiq85/collision/outer_finger.dae"/>
        </geometry>
      </collision>
    </link>

    <link name="${prefix}right_inner_finger">
      <inertial>
        <origin xyz="0.000299999999999317 0.0160078233491243 -0.0136945669206257"/>
        <mass value="0.0104003125914103"/>
        <inertia ixx="2.71909453810972E-06" ixy="1.35402465472579E-21" ixz="-7.1817349065269E-22"
                 iyy="7.69100314106116E-07" iyz="6.74715432769696E-07" izz="2.30315190420171E-06"/>
      </inertial>
      <visual>
        <geometry>
          <mesh filename="file://$(find robotiq_description)/meshes/robotiq85/visual/inner_finger.dae"/>
        </geometry>
      </visual>
      <collision>
        <geometry>
          <mesh filename="file://$(find robotiq_description)/meshes/robotiq85/collision/inner_finger.dae"/>
        </geometry>
      </collision>
    </link>

    <link name="${prefix}right_inner_finger_pad">
      <visual>
        <geometry>
          <box size="0.022 0.00635 0.0375"/>
        </geometry>
        <material name="white">
          <color rgba="0.9 0.9 0.9 1"/>
        </material>
      </visual>
      <collision>
        <geometry>
          <box size="0.022 0.00635 0.0375"/>
        </geometry>
      </collision>
    </link>

    <link name="${prefix}right_inner_knuckle">
      <inertial>
        <origin xyz="0.000123011831763771 0.0507850843201817 0.00103968640075166"/>
        <mass value="0.0271177346495152"/>
        <inertia ixx="2.61910379223783E-05" ixy="-2.43616858946494E-07" ixz="-6.37789906117123E-09"
                 iyy="2.8270243746167E-06" iyz="-5.37200748039765E-07" izz="2.83695868220296E-05"/>
      </inertial>
      <visual>
        <geometry>
          <mesh filename="file://$(find robotiq_description)/meshes/robotiq85/visual/inner_knuckle.dae"/>
        </geometry>
      </visual>
      <collision>
        <geometry>
          <mesh filename="file://$(find robotiq_description)/meshes/robotiq85/collision/inner_knuckle.dae"/>
        </geometry>
      </collision>
    </link>

    <!-- Main actuated joint - left outer knuckle -->
    <joint name="${prefix}gripper_joint" type="revolute">
      <origin rpy="0 0 3.14159265359" xyz="0 -0.0306011 0.054904"/>
      <parent link="${prefix}gripper_base"/>
      <child link="${prefix}left_outer_knuckle"/>
      <axis xyz="1 0 0"/>
      <limit effort="1000" lower="0" upper="0.81" velocity="2.0"/>
    </joint>

    <joint name="${prefix}left_outer_finger_joint" type="fixed">
      <origin xyz="0 0.0315 -0.0041"/>
      <parent link="${prefix}left_outer_knuckle"/>
      <child link="${prefix}left_outer_finger"/>
      <axis xyz="1 0 0"/>
    </joint>

    <!-- Left inner knuckle mimics the outer knuckle with a mechanical coupling -->
    <joint name="${prefix}left_inner_knuckle_joint" type="revolute">
      <origin rpy="0 0 3.14159265359" xyz="0 -0.0127 0.06142"/>
      <parent link="${prefix}gripper_base"/>
      <child link="${prefix}left_inner_knuckle"/>
      <axis xyz="1 0 0"/>
      <limit effort="1000" lower="0" upper="0.8757" velocity="2.0"/>
      <mimic joint="${prefix}gripper_joint" multiplier="1" offset="0"/>
    </joint>

    <!-- Left inner finger has opposite motion to simulate the mechanical linkage -->
    <joint name="${prefix}left_inner_finger_joint" type="revolute">
      <origin xyz="0 0.0061 0.0471"/>
      <parent link="${prefix}left_outer_finger"/>
      <child link="${prefix}left_inner_finger"/>
      <axis xyz="1 0 0"/>
      <limit effort="1000" lower="-0.8757" upper="0" velocity="2.0"/>
      <mimic joint="${prefix}gripper_joint" multiplier="-1" offset="0"/>
    </joint>

    <joint name="${prefix}left_inner_finger_pad_joint" type="fixed">
      <origin xyz="0 -0.0220203446692936 0.03242"/>
      <parent link="${prefix}left_inner_finger"/>
      <child link="${prefix}left_inner_finger_pad"/>
      <axis xyz="0 0 1"/>
    </joint>

    <!-- Right side joints mimic the left side with opposite motion -->
    <joint name="${prefix}right_outer_knuckle_joint" type="revolute">
      <origin xyz="0 0.0306011 0.054904"/>
      <parent link="${prefix}gripper_base"/>
      <child link="${prefix}right_outer_knuckle"/>
      <axis xyz="1 0 0"/>
      <limit effort="1000" lower="0" upper="0.81" velocity="2.0"/>
      <mimic joint="${prefix}gripper_joint" multiplier="1" offset="0"/>
    </joint>

    <joint name="${prefix}right_outer_finger_joint" type="fixed">
      <origin xyz="0 0.0315 -0.0041"/>
      <parent link="${prefix}right_outer_knuckle"/>
      <child link="${prefix}right_outer_finger"/>
    </joint>

    <joint name="${prefix}right_inner_knuckle_joint" type="revolute">
      <origin rpy="0 0 0.0" xyz="0 0.0127 0.06142"/>
      <parent link="${prefix}gripper_base"/>
      <child link="${prefix}right_inner_knuckle"/>
      <axis xyz="1 0 0"/>
      <limit effort="1000" lower="0" upper="0.8757" velocity="2.0"/>
      <mimic joint="${prefix}gripper_joint" multiplier="1" offset="0"/>
    </joint>

    <joint name="${prefix}right_inner_finger_joint" type="revolute">
      <origin xyz="0 0.0061 0.0471"/>
      <parent link="${prefix}right_outer_finger"/>
      <child link="${prefix}right_inner_finger"/>
      <axis xyz="1 0 0"/>
      <limit effort="1000" lower="-0.8757" upper="0" velocity="2.0"/>
      <mimic joint="${prefix}gripper_joint" multiplier="-1" offset="0"/>
    </joint>

    <joint name="${prefix}right_inner_finger_pad_joint" type="fixed">
      <origin xyz="0 -0.0220203446692936 0.03242"/>
      <parent link="${prefix}right_inner_finger"/>
      <child link="${prefix}right_inner_finger_pad"/>
    </joint>
  </xacro:macro>
</robot>